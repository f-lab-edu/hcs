{
  "compilerOptions": {
    "esModuleInterop": true,
    "sourceMap": true,
    "allowJs": true,
    "lib": [
      "DOM",
      "ES6",
      "esnext",
      "DOM.Iterable",
      "ScriptHost",
      "ES2016.Array.Include"
    ],
    "jsx": "react",
    "module": "esnext",
    "moduleResolution": "Node",
    "target": "es6",
    "strict": true,
    "resolveJsonModule": true,
    "noResolve": false,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": false,
    "strictNullChecks": true,
    "baseUrl": ".",
    "removeComments": true,
    "paths": {
      "@hooks/*": [
        "hooks/*"
      ],
      "@components/*": [
        "components/*"
      ],
      "@context/*": [
        "context/*"
      ],
      "@layouts/*": [
        "layouts/*"
      ],
      "@pages/*": [
        "pages/*"
      ],
      "@utils/*": [
        "utils/*"
      ],
      "@typings/*": [
        "typings/*"
      ],
      "@reducers/*": [
        "reducers/*"
      ],
      "@actions/*": [
        "actions/*"
      ],
      "@store/*": [
        "store/*"
      ]
    }
  }
}
