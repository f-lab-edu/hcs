<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hcs.mapper.ChatRoomMapper">

    <insert id="insertChatRoom" useGeneratedKeys="true" keyProperty="id" parameterType="com.hcs.domain.ChatRoom">
        insert into ChatRoom (id, createdAt)
        values (#{id}, #{createdAt})
    </insert>

</mapper>
